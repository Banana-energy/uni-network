{
  "name": "@uni-helper/uni-network",
  "version": "0.18.0",
  "description": "为 uni-app 打造的基于 Promise 的 HTTP 客户端",
  "keywords": [
    "uni-app",
    "uniapp",
    "uni",
    "request",
    "network",
    "upload",
    "uploadFile",
    "download",
    "downloadFile"
  ],
  "homepage": "https://github.com/uni-helper/uni-network#readme",
  "bugs": {
    "url": "https://github.com/uni-helper/uni-network/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uni-helper/uni-network.git"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui-dev@foxmail.com",
    "url": "https://modyqyw.github.io"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./composables": {
      "import": {
        "types": "./dist/composables.d.mts",
        "default": "./dist/composables.mjs"
      },
      "require": {
        "types": "./dist/composables.d.cts",
        "default": "./dist/composables.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "commit": "git-cz",
    "depupdate": "taze -f",
    "dev": "unbuild --stub",
    "docs:build": "vitepress build docs",
    "docs:dev": "vitepress dev docs",
    "docs:preview": "vitepress preview docs",
    "format": "prettier . \"!**/packages-lock.json*\" \"!**/yarn.lock\" \"!**/pnpm-lock.yaml\" --ignore-unknown --write --cache --log-level=warn",
    "lint": "conc \"pnpm:lint:script\" \"pnpm:lint:markdownlint\"",
    "lint:eslint": "eslint ./src --fix --cache",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:oxlint": "oxlint ./src/ --deny=correctness --deny=perf --fix",
    "lint:script": "nr lint:oxlint && nr lint:eslint",
    "prepare": "is-ci || simple-git-hooks",
    "prepublishOnly": "pnpm run build",
    "release": "pnpm install && pnpm run lint && pnpm run typecheck && bumpp",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@dcloudio/types": "^3.4.7",
    "@types/lodash.merge": "^4.6.9",
    "@types/statuses": "^2.0.4",
    "fast-querystring": "^1.1.2",
    "lodash.merge": "^4.6.2",
    "statuses": "^2.0.1",
    "vue-demi": "^0.14.7"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.12",
    "@babel/core": "^7.23.9",
    "@babel/eslint-parser": "^7.23.10",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@commitlint/prompt": "^18.6.1",
    "@modyqyw/fabric": "^10.3.8",
    "@modyqyw/utils": "^8.0.0",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.19",
    "@types/ungap__url-search-params": "^0.1.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@ungap/url-search-params": "^0.2.2",
    "@vitest/coverage-v8": "^1.3.0",
    "@vitest/ui": "^1.3.0",
    "@vueuse/core": "^10.7.2",
    "bumpp": "^9.3.0",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.2",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.56.0",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.2",
    "markdownlint-cli": "^0.39.0",
    "oxlint": "^0.2.9",
    "prettier": "^3.2.5",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.13.3",
    "tsx": "^4.7.1",
    "type-fest": "^4.10.2",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vitepress": "1.0.0-rc.43",
    "vitest": "^1.3.0",
    "vue": "^3.4.19"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0",
    "@vueuse/core": "^9.0.0 || ^10.0.0",
    "vue": "^2.6.0 || ^2.7.0 || ^3.2.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    },
    "@vueuse/core": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "packageManager": "pnpm@8.15.3",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
